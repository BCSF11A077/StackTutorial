@Model List<stacktutorial.models.user>
   
               @{
                   ViewBag.Title = "Admin";
               }


    <h3>Admin Panel</h3>
    <h2>List of Users</h2>

               <table data-toggle="table" class="table table-bordered">
                  <thead> <tr>
                       <td>UID</td>
                       <td>Username</td>
                   </tr>
                   </thead>
                   @foreach (var s in Model)
                   {
                       <tbody>
                       <tr>
                           <td>@s.Id</td>
                           <td>@s.email</td>

                           <td>

                               <a href="/Admin/Delete/@s.Id">Delete</a>
                           </td>
                       </tr></tbody>
                   }
               </table>

    <a href="/Admin/Add/">Add User</a>
